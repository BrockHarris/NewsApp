<h1>News Feed:</h1>
<% parsely %>
<% @presult.each do |presult| %>
	<% purl = presult['url']%>
	<% ptitle = presult['title'] %>
	<% pthumb = presult['thumb_url_medium'] %>
	<%= sentiment_return(ptitle, purl, pthumb) %>
<% end %>
<br />
<% get_data("highvalue") %>
<% @result.each do |result| %>
	<% page = MetaInspector.new(result) %>
	<%= sentiment_return(page.title, result, page.image)%>
	<br />
<% end %>
<br />
<h3>Settings:</h3>
 

<div class="image_tile">
</div>

